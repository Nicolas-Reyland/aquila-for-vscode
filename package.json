{
  "name": "aquila-for-vscode",
  "version": "1.1.2",
  "engines": {
    "vscode": ">=0.9.0-pre.1"
  },
  "publisher": "NicolasReyland",
  "description": "Aquila grammar package (syntax highlighting)",
  "contributes": {
  	"languages": [{
  		"id": "aquila",
