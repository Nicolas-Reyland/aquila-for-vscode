  	}]
  },
  "icon": "images/aquila-icon.png",

  "author": "Nicolas Reyland",
  "description": "Aquila grammar package (syntax highlighting)",
  "repository": "https://github.com/Nicolas-Reyland/aquila-for-vscode",
  "keywords": [
    "syntax",
    "highlighting",
    "grammar"
  ],
  "license": "MIT",
  "bugs": "https://github.com/Nicolas-Reyland/aquila-for-vscode/issues"
